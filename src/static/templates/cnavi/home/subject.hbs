<th>
	<div class="icons-draggable sprite drag-icon"></div>
	<div class="icons-favorite_{{#if registration.favorite }}on{{else}}off{{/if}} sprite favorite-icon"></div>
	<a href="#">{{ trObj registration.subject 'name' }}</a>
</th>
<td>{{ registration.period.year }}</td>
<td>
    {{#each registration.subject.classes}}
        {{#if termNeedPrint}}
            <span>{{ tr "cnavi" "courses.info.seasons" term.name }}</span>
        {{/if}}
    {{/each}}
</td>
<td>
    {{#each registration.subject.classes}}
    <span>{{classTime this}}</span>
    {{/each}}
</td>
<td>{{ trObj registration.subject.school 'name' }}</td>
